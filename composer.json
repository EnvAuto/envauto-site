{
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/" },
		{ "type": "vcs", "url":"git@github.com:EnvAuto/site-package-bootstrap"},
		{ "type": "vcs", "url":"git@github.com:EnvAuto/car-list-extension.git"}
	],
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0+",
	"require": {
		"typo3/cms": "^8.7",
		"envauto/envauto-bootstrap-site-package":"dev-master",
		"envauto/car-list":"dev-master",
		"helhum/typo3-console": "^4.4"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		}
	},
	"require-dev":{
		"typo3/cms-tstemplate":"^8.7",
		"typo3/cms-t3editor":"^8.7",
		"typo3/cms-beuser":"^8.7",
		"friendsoftypo3/extension-builder":"^8.7",
		"codeception/codeception": "^2.3",
		"phpunit/phpunit": "^6.4",
		"typo3/testing-framework": "^2.0"
	}
}
